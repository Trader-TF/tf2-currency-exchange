{
  "name": "tf2-currency-exchange",
  "version": "1.1.0",
  "description": "Easy to use classes to make picking currencies for trading easy!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "semantic-release": "semantic-release",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "npx prettier -w \"./src/**.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trader-TF/tf2-currency-exchange.git"
  },
  "keywords": [],
  "author": "danocmx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Trader-TF/tf2-currency-exchange/issues"
  },
  "homepage": "https://github.com/Trader-TF/tf2-currency-exchange#readme",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "jest": "^27.1.1",
    "prettier": "^2.3.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "tf2-currency": "^1.1.1"
  }
}
